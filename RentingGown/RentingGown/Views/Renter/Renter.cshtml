<body onload="afterLoad()">
    <h2>ברוכים הבאים</h2>
    @*@{ if (Session["user"] != null)
            {
                <span>@{(Session["user"] as RentingGown.Models.Renters).fname}</span>
        }

        }*@
    <input type="hidden" id="msg" name=@ViewBag.msg />
