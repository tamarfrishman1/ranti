<script>
    function afterLoad() {
        if (document.getElementById("msg").name == "1")
            swal({
                icon: 'success',
                title: 'מצוין',
                text: 'השמלה נוספה בהצלחה.'
            })
    }
</script>
<script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
